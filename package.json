{
  "name": "@aiworkspace/shared-header",
  "version": "1.0.0",
  "description": "Shared header component for AIWorkspace small apps with authentication, workspace management, and cross-subdomain support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "element-plus": "^2.0.0",
    "pinia": "^2.0.0 || ^3.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "pinia": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@vitejs/plugin-vue": "^4.3.0",
    "@vue/tsconfig": "^0.4.0",
    "element-plus": "^2.4.0",
    "pinia": "^2.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "keywords": [
    "vue",
    "header",
    "component",
    "aiworkspace",
    "authentication",
    "workspace",
    "supabase",
    "cross-subdomain"
  ],
  "author": "Vikas Kedia <kedia.vikas@gmail.com>",
  "license": "MIT"
}
